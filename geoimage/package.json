  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "rollup": "rollup -c",
    "start": "yarn && yarn rollup",
